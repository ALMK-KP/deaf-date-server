// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Track {
  id Int @id @default(autoincrement())
  ytId String
  ytLink String
  playlistId String?
  thumbnail String
  title String
  customTitle String
  audio String
}
